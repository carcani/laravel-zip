{
  "name": "zanysoft/laravel-zip",
  "description": "laravel-zip is the world's leading zip utility for file compression and backup.",
  "keywords": [
    "laravel-zip",
    "laravel",
    "laravel8",
    "laravel9",
    "zip",
    "unzip",
    "extract",
    "backup",
    "ZipArchive",
    "multiple",
    "merge"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Zany Soft",
      "email": "zanysoft@outlook.com"
    }
  ],
  "support": {
    "email": "zanysoft@outlook.com"
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "scrutinizer/ocular": "^1.9",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "ZanySoft\\Zip\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanySoft\\Zip\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\Zip\\ZipServiceProvider"
      ],
      "aliases": {
        "Zip": "ZanySoft\\Zip\\Facades\\Zip"
      }
    }
  }
}
